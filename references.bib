@Article{Lee006395,
  author       = {Lee, Hayan and Gurtowski, James and Yoo, Shinjae and Marcus, Shoshana and McCombie, W. Richard and Schatz, Michael},
  journal      = {bioRxiv},
  title        = {{Error correction and assembly complexity of single molecule sequencing reads}},
  year         = {2014},
  abstract     = {Third generation single molecule sequencing technology is poised to revolutionize genomics by enabling the sequencing of long, individual molecules of DNA and RNA. These technologies now routinely produce reads exceeding 5,000 basepairs, and can achieve reads as long as 50,000 basepairs. Here we evaluate the limits of single molecule sequencing by assessing the impact of long read sequencing in the assembly of the human genome and 25 other important genomes across the tree of life. From this, we develop a new data-driven model using support vector regression that can accurately predict assembly performance. We also present a novel hybrid error correction algorithm for long PacBio sequencing reads that uses pre-assembled Illumina sequences for the error correction. We apply it several prokaryotic and eukaryotic genomes, and show it can achieve near-perfect assemblies of small genomes (\&lt; 100Mbp) and substantially improved assemblies of larger ones. All source code and the assembly model are available open-source.Abbreviationsbpbase pairGbpgigabasesMbpmegabasesSNPsingle nucleotide polymorphism},
  doi          = {10.1101/006395},
