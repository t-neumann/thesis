@Article{Chang2019,
  author          = {Chang, Guoqiang and Leu, Jia-Shiun and Ma, Li and Xie, Keping and Huang, Suyun},
  journal         = {Cytokine},
  title           = {{Methylation of RNA N$^6$-methyladenosine in modulation of cytokine responses and tumorigenesis.}},
  year            = {2019},
  issn            = {1096-0023},
  month           = jun,
  pages           = {35--41},
  volume          = {118},
  abstract        = {Among myriads of distinct chemical modification in RNAs, the dynamic, reversible and fine-tuned methylation of N -methyladenosine (m A) is the most prevalent modification in eukaryotic mRNAs. This RNA mark is generated by proteins that act as m A writers and can be reversed by proteins that act as m A erasers. The RNA m A modification is also mediated by another group of proteins capable of recognizing m A that act as m A readers. The m A modification exerts direct control over the RNA metabolism including mRNA processing, mRNA exporting, translation initiation, mRNA stability and the biogenesis of long-non-coding RNA (LncRNA), thereby can influence various aspects of cell function. Evidently, m A is intimately associated with cancer development and progression such as self-renewal capacity of cancer stem cells, proliferation, apoptosis and therapeutic resistance, and immune response. In this review, we will discuss the regulation and function of m A, the various functions ascribed to these proteins and the emerging concepts that impact our knowledge of these proteins and their roles in the epitranscriptome. Conceivably, m A may play pivotal roles in cytokine and immune response and carcinogenesis.},
