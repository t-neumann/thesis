  keywords        = {Gene Expression Profiling; HEK293 Cells; HeLa Cells; Humans; RNA-Binding Proteins, genetics, metabolism; Transcriptome, physiology; RNA; RNA–protein interactions; bioorthogonal chemistry; noncoding RNA; proteomics},
  nlm-id          = {7505876},
  owner           = {NLM},
  pii             = {1718406115},
