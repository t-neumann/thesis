@Article{Mauer2017,
  author          = {Mauer, Jan and Luo, Xiaobing and Blanjoie, Alexandre and Jiao, Xinfu and Grozhik, Anya V. and Patil, Deepak P. and Linder, Bastian and Pickering, Brian F. and Vasseur, Jean-Jacques and Chen, Qiuying and Gross, Steven S. and Elemento, Olivier and Debart, Françoise and Kiledjian, Megerditch and Jaffrey, Samie R.},
  journal         = {Nature},
  title           = {{Reversible methylation of m$^6$A$_m$ in the 5' cap controls mRNA stability.}},
  year            = {2017},
  issn            = {1476-4687},
  month           = jan,
  pages           = {371--375},
  volume          = {541},
  abstract        = {Internal bases in mRNA can be subjected to modifications that influence the fate of mRNA in cells. One of the most prevalent modified bases is found at the 5' end of mRNA, at the first encoded nucleotide adjacent to the 7-methylguanosine cap. Here we show that this nucleotide, N ,2'-O-dimethyladenosine (m A ), is a reversible modification that influences cellular mRNA fate. Using a transcriptome-wide map of m A  we find that m A -initiated transcripts are markedly more stable than mRNAs that begin with other nucleotides. We show that the enhanced stability of m A -initiated transcripts is due to resistance to the mRNA-decapping enzyme DCP2. Moreover, we find that m A  is selectively demethylated by fat mass and obesity-associated protein (FTO). FTO preferentially demethylates m A  rather than N -methyladenosine (m A), and reduces the stability of m A  mRNAs. Together, these findings show that the methylation status of m A  in the 5' cap is a dynamic and reversible epitranscriptomic modification that determines mRNA stability.},
  chemicals       = {MicroRNAs, RNA Caps, mRNA decapping enzymes, Guanosine, 7-methylguanosine, N(6),O(2)-dimethyladenosine, N-methyladenosine, Alpha-Ketoglutarate-Dependent Dioxygenase FTO, FTO protein, human, Endoribonucleases, Adenosine},
