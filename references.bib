@Article{Chen2022,
  author          = {Chen, Hong-Xuan and Zhang, Zhang and Ma, Dong-Zhao and Chen, Li-Qian and Luo, Guan-Zheng},
  journal         = {Methods (San Diego, Calif.)},
  title           = {{Mapping single-nucleotide m$^6$A by m$^6$A-REF-seq.}},
  year            = {2022},
  issn            = {1095-9130},
  month           = jul,
  pages           = {392--398},
  volume          = {203},
  abstract        = {The past few years have witnessed rapid progress in the field of RNA modifications. As the most prevailing modification on eukaryotic mRNA, m A is characterized to play a vital role in various cellular activities. However, limitations of the detection method impede functional studies of m A. Here we introduce m A-REF-seq, a powerful and straightforward method to identify m A at single-nucleotide resolution. m A-REF-seq relies on the recognition of RNA endonuclease MazF towards m A at the ACA motif, providing an orthogonal method independent of the m A antibody being adopted by most of current methods. We describe a detailed protocol to perform m A-REF-seq, including NGS library construction and sequencing data analysis. In particular, we describe an optimized assay to validate individual m A sites identified by m A-REF-seq, which can also be applied to detect any candidate m A sites.},
